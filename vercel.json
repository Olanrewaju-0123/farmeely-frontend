{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/simple-test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/minimal.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/minimal",
      "dest": "api/minimal.js"
    },
    {
      "src": "/simple-test",
      "dest": "api/simple-test.js"
    },
    {
      "src": "/test",
      "dest": "api/test.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "installCommand": "rm -rf node_modules package-lock.json && npm install",
  "buildCommand": "echo 'Backend deployment - dependencies installed'"
}
