{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "installCommand": "rm -rf node_modules package-lock.json && npm install",
  "buildCommand": "echo 'Backend deployment - dependencies installed'"
}
